a = 0
print("What is your name?")
name = input()
print("Hello, " + name + "!")
print("How are you doing today?")
feeling = input()
if feeling.lower() in ["good", "great", "fine", "well", "awesome"]:
    print("That's wonderful to hear, " + name + "!")
    a += 1
elif feeling.lower() in ["bad", "not good", "sad", "terrible", "awful"]:
    print("I'm sorry to hear that, " + name + ". I hope things get better soon.")
    a += 1
else:
    print("Thanks for sharing, " + name + ".")
print("What did you want to talk about today?")
topic = input()
print("Interesting! Let's talk about " + topic + ".")
topic2 = input("is " + topic + " something you enjoy?")
if topic2.lower() in ["yes", "yeah", "yep", "sure", "absolutely"]:
    print("That's great! It's always nice to talk about things we enjoy.")
    a += 1
elif topic2.lower() in ["no", "not really", "nah", "nope"]:
    print("That's okay! Sometimes it's good to explore new topics.")
    a -= 1
    print("Would you like to talk about something else?")
    response = input()
    if response.lower() in ["yes", "yeah", "sure"]:
        print("What would you like to talk about?")
        new_topic = input()
        print("Awesome! Let's discuss " + new_topic + ".")
        a += 1
else:
    print("Thanks for your input!")
print("Do you need any help with anything else, " + name + "?")
help_response = input()
if help_response.lower() in ["yes", "yeah", "sure"]:
    print("What do you need help with?")
    help_topic = input()
    print("I'll do my best to assist you with " + help_topic + ".")
    a += 1
    if help_topic.lower() in ["coding", "programming", "python"]:
        print("I can definitely help you with coding! What specific issue are you facing?")
        coding_issue = input()
        print("Let's work through your coding issue: " + coding_issue)
        if coding_issue.lower() in ["syntax error", "bug", "crash"]:
            print("Have you tried checking your code for typos or running a debugger?")
            coding_issue_response = input()
            if coding_issue_response.lower() in ["yes", "i have", "already did"]:
                print("Great! If the issue persists, consider seeking help from online coding communities.")
                a += 1
            elif coding_issue_response.lower() in ["no", "not yet"]:
                print("I recommend trying those steps first. They often resolve common coding issues.")
                a += 1
            print("glad I could help with your coding issue, " + name + "!")
    elif help_topic.lower() in ["math", "science", "history", "Miscellaneous"]:
        print("I can help with that too! What specific question do you have?")
        subject_question = input()
        print("Let's explore your question about " + subject_question + ".")
        if subject_question.lower() in ["science"]:
            print("Science is fascinating! What area of science do you need help with?")
